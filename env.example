# Polygon (US options)
POLYGON_API_KEY=your-polygon-key
POLYGON_BASE_URL=https://api.polygon.io

# MarketData.app (NBBO-like options quotes)
MARKETDATA_API_KEY=your-marketdata-token
MARKETDATA_BASE_URL=https://api.marketdata.app
# AiiQ_tAIq Environment Configuration
# Copy this file to .env and fill in your values

# Frontend (Next.js)
NEXT_PUBLIC_API_BASE=http://127.0.0.1:8080
NEXT_PUBLIC_WS_BASE=ws://127.0.0.1:8080/ws
NEXT_PUBLIC_SOLANA_CLUSTER=mainnet-beta
NEXT_PUBLIC_HELIUS_RPC=https://mainnet.helius-rpc.com/?api-key=your-helius-api-key

# Orchestrator Service
# For real market data, set DISABLE_DERIBIT=0 and ensure internet access
DERIBIT_BASE_URL=wss://www.deribit.com/ws/api/v2
PAIT_LOG_URL=http://127.0.0.1:8000/api/claudia/pait/log
RISK_LIMIT_DEFAULT="Max 1% per trade; VaR<=5%"
PORT=8080
# 0 to enable real Deribit WebSocket; 1 keeps mocked data
DISABLE_DERIBIT=0

# Solana Adapter Service
HELIUS_RPC=https://mainnet.helius-rpc.com/?api-key=your-helius-api-key
SOLANA_CLUSTER=mainnet-beta
ZETA_PROGRAM_ID=ZetaSo111111111111111111111111111111111111111
PSY_PROGRAM_ID=PsyFiZVj6osQfYjsugmpHcdGBeKTtZxVj6Rh3UfE9Nf
ORCH_WS_SINK=ws://127.0.0.1:8080/ingest/solana

# Ollama (local LLM)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=claudia-trader:latest

# Database (Optional)
DATABASE_URL=postgresql://aiiq_user:aiiq_password@localhost:5432/aiiq_trading
REDIS_URL=redis://localhost:6379

# Claudia API (Optional)
CLAUDIA_BASE_URL=http://127.0.0.1:8000
CLAUDIA_API_KEY=your-claudia-api-key

# Development
NODE_ENV=development
DEBUG=true
